#ifndef SDP_FUNC_VERSION_H_
#define SDP_FUNC_VERSION_H_

/**
 * @def SDP_FUNC_VERSION
 *
 * @brief SDP_FUNC_VERSION is a macro used to determine the version of the
 * library.
 *
 * @details
 * This macro expands to a numeric value of the form 0xMMNNPP (MM = major,
 * NN = minor, PP = patch) that specifies the version number of the library.
 * For example, in version 2.1.3 this would expand to 0x020103.
 * NOTE this value is set in the CMake build system
 */
#define SDP_FUNC_VERSION @SDP_FUNC_VERSION_ID@

/**
 * @def SDP_FUNC_VERSION_STR
 *
 * @brief SDP_FUNC_VERSION_STR is a macro used to return the version of the
 * library as a text string.
 *
 * @details
 * This macro expands to a string that specifies the version number
 * (for example, "2.1.3").
 * NOTE this value is set in the CMake build system
 */
#define SDP_FUNC_VERSION_STR "@SDP_FUNC_VERSION_STR@"

#endif /* include guard */
